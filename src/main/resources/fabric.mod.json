{
  "schemaVersion": 1,
  "id": "eer",
  "version": "${version}",
  "name": "${readable_name}",
  "description": "${mod_description}",
  "authors": [
    "The_Fireplace"
  ],
  "contact": {
    "homepage": "${fabric_curseforge_url}",
    "sources": "${github_url}",
    "issues": "${github_url}/issues",
    "modrinth": "${modrinth_url}"
  },
  "license": "${license_id}",
  "icon": "${logo_filename}",
  "environment": "*",
  "entrypoints": {
  },
  "mixins": [
    "eer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.0",
    "minecraft": ">=1.18"
  },

  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord_url}",
        "eer.twitter": "${twitter_url}",
        "eer.kofi": "${kofi_url}",
        "eer.patreon": "${patreon_url}"
      }
    }
  }
}
